<%= render :partial => 'admin/shared/configuration_menu' %>

<h1><%= t("editing_gateway") %></h1>
<%= error_messages_for :gateway %>
<% form_for(@gateway, :url => object_url, :html => { :method => :put }) do |f| %>
  <%= render :partial => "form", :locals => { :f => f } %>
  <p>
    <%=submit_tag t("update")%>
  </p>
<% end %>